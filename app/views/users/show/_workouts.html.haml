- unless @presenter.workouts.nil?
  %table.table
    %tr
      %thead
        %th Workout
        %th
  
    - for workout in @presenter.workouts.first(5)
  
      %tr
        %td= link_to "#{workout.title}", workout_path(workout)
        %td= link_to "Add to Booking", new_booking_path(workout: workout)